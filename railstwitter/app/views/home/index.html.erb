<div class="row">
    <h1>Tweets Collected</h1>
</div>
<div class="row">
    <div id="tweetCounter" class="well col-sm-4 col-sm-offset-4"><%= @tweet %></div>
</div>

<div class="row">
    <div class="col-sm-2">
        <%= link_to "View My Tweets", :home_view , :class => "btn btn-primary"%>
    </div>
    
    <div class="well col-sm-8 col-sm-offset-1">
        <div id="lastTweet">
            <%= @last.tweettext %> -<%= @last.username %>
        </div>
    </div>
</div>


